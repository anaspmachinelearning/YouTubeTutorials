     ]
    }
   ],
   "source": [
    "#Shallow copy - 1 level deep\n",
    "\n",
    "first_list = [[1,2,3],'a','b','c']\n",
    "\n",
    "second_list = first_list.copy()\n",
    "\n",
    "first_list[0][2] = 999\n",
    "\n",
    "print(first_list)\n",
    "\n",
    "print(second_list)"
   ]
  },
